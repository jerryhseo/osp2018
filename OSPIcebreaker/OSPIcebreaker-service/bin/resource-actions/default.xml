<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action Mapping 7.0.0//EN" "http://www.liferay.com/dtd/liferay-resource-action-mapping_7_0_0.dtd">

<resource-action-mapping>
	<model-resource>
	    <model-name>com.kisti.osp.icebreaker</model-name>
	    <portlet-ref>
	    	<portlet-name>OSPSchedulerManager</portlet-name>
	    </portlet-ref>
	    <permissions>
	        <supports>
	            <action-key>ADD_ENTRY</action-key>
	            <action-key>DELETE</action-key>
	            <action-key>UPDATE</action-key>
	            <action-key>VIEW</action-key>
	        </supports>
	        <site-member-defaults>
	        	<action-key>VIEW</action-key>
	        </site-member-defaults>
	        <guest-defaults />
	        <guest-unsupported>
	            <action-key>ADD_ENTRY</action-key>
	            <action-key>DELETE</action-key>
	            <action-key>UPDATE</action-key>
	        </guest-unsupported>
	    </permissions>
	</model-resource>
	
	<model-resource>
	    <model-name>com.kisti.osp.icebreaker.model.OSPSchedulerEntry</model-name>
	    <portlet-ref>
	    	<portlet-name>OSPSchedulerManager</portlet-name>
	    </portlet-ref>
	    <permissions>
	        <supports>
	            <action-key>ADD_ENTRY</action-key>
	            <action-key>DELETE</action-key>
	            <action-key>PERMISSIONS</action-key>
	            <action-key>UPDATE</action-key>
	            <action-key>VIEW</action-key>
	        </supports>
	        <site-member-defaults>
	            <action-key>VIEW</action-key>
	        </site-member-defaults>
	        <guest-defaults>
	            <action-key>VIEW</action-key>
	        </guest-defaults>
	        <guest-unsupported>
	            <action-key>ADD_ENTRY</action-key>
	            <action-key>DELETE</action-key>
	            <action-key>PERMISSIONS</action-key>
	            <action-key>UPDATE</action-key>
	        </guest-unsupported>
	    </permissions>
	</model-resource>
	
	<model-resource>
	    <model-name>com.kisti.osp.icebreaker.model.Job</model-name>
	    <portlet-ref>
	    	<portlet-name>OSPJobMonitor</portlet-name>
	    </portlet-ref>
	    <permissions>
	        <supports>
	            <action-key>ADD_ENTRY</action-key>
	            <action-key>DELETE</action-key>
	            <action-key>CANCEL</action-key>
	            <action-key>SUSPEND</action-key>
	            <action-key>RESTART</action-key>
	            <action-key>PERMISSIONS</action-key>
	            <action-key>UPDATE</action-key>
	            <action-key>RUN</action-key>
	            <action-key>RERUN</action-key>
	            <action-key>VIEW</action-key>
	        </supports>
	        <site-member-defaults>
	            <action-key>ADD_ENTRY</action-key>
	            <action-key>DELETE</action-key>
	            <action-key>CANCEL</action-key>
	            <action-key>SUSPEND</action-key>
	            <action-key>RESTART</action-key>
	            <action-key>PERMISSIONS</action-key>
	            <action-key>UPDATE</action-key>
	            <action-key>RUN</action-key>
	            <action-key>RERUN</action-key>
	            <action-key>VIEW</action-key>
	        </site-member-defaults>
	        <guest-defaults>
	            <action-key>VIEW</action-key>
	        </guest-defaults>
	        <guest-unsupported>
	            <action-key>ADD_ENTRY</action-key>
	            <action-key>DELETE</action-key>
	            <action-key>CANCEL</action-key>
	            <action-key>SUSPEND</action-key>
	            <action-key>RESTART</action-key>
	            <action-key>PERMISSIONS</action-key>
	            <action-key>UPDATE</action-key>
	            <action-key>RUN</action-key>
	            <action-key>RERUN</action-key>
	        </guest-unsupported>
	    </permissions>
	</model-resource>
</resource-action-mapping>